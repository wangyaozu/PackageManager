    <Sign_TimestampUrl>http://timestamp.digicert.com</Sign_TimestampUrl>
    <Sign_CertPath></Sign_CertPath>
    <Sign_CertPassword></Sign_CertPassword>
    <Sign_SubjectName></Sign_SubjectName>
  </PropertyGroup>
  <Target Name="AuthenticodeSign" AfterTargets="AfterBuild" Condition="'$(Sign_Enabled)'=='true'">
    <Message Text="Signing $(TargetPath)" Importance="high" />
    <Exec Condition="'$(Sign_CertPath)' != ''" Command="&quot;$(SignToolExe)&quot; sign /f &quot;$(Sign_CertPath)&quot; /p &quot;$(Sign_CertPassword)&quot; /tr &quot;$(Sign_TimestampUrl)&quot; /td SHA256 /fd SHA256 &quot;$(TargetPath)&quot;" />
    <Exec Condition="'$(Sign_CertPath)' == '' AND '$(Sign_SubjectName)' != ''" Command="&quot;$(SignToolExe)&quot; sign /n &quot;$(Sign_SubjectName)&quot; /tr &quot;$(Sign_TimestampUrl)&quot; /td SHA256 /fd SHA256 &quot;$(TargetPath)&quot;" />
    <Exec Condition="'$(Sign_CertPath)' == '' AND '$(Sign_SubjectName)' == ''" Command="echo No signing certificate configured. Skipping signing." />
    <Exec Command="&quot;$(SignToolExe)&quot; verify /pa &quot;$(TargetPath)&quot;" />
  </Target>
